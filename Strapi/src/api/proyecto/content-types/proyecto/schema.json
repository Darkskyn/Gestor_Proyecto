{
  "kind": "collectionType",
  "collectionName": "proyectos",
  "info": {
    "singularName": "proyecto",
    "pluralName": "proyectos",
    "displayName": "Proyecto",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "Nombre_Proyecto": {
      "type": "string",
      "required": false,
      "maxLength": 100,
      "minLength": 5
    },
    "Descripcion": {
      "type": "text",
      "required": false,
      "maxLength": 250,
      "minLength": 20,
      "private": true
    },
    "Fecha_Inicio": {
      "type": "date",
      "required": false
    },
    "Fecha_Fin": {
      "type": "date",
      "required": false
    },
    "Estado_Proyecto": {
      "type": "enumeration",
      "enum": [
        "Pendiente de Aprobaci√≥n",
        "En Progreso",
        "Completado",
        "Cancelado",
        "Suspendido"
      ],
      "required": false
    },
    "Departamento": {
      "type": "enumeration",
      "enum": [
        "Requerimiento",
        "Proyecto"
      ],
      "required": false
    },
    "Objetivo_Proyecto": {
      "type": "text",
      "required": false,
      "maxLength": 250,
      "minLength": 20
    },
    "Prioridad": {
      "type": "enumeration",
      "enum": [
        "Baja",
        "Media",
        "Alta"
      ],
      "required": false
    },
    "Gerente_Proyecto": {
      "type": "enumeration",
      "enum": [
        "Jesus Garcia",
        "Augusto Grado"
      ],
      "required": false
    },
    "Id_tareas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tarea.tarea",
      "mappedBy": "Id_proyecto"
    },
    "Presupuesto": {
      "type": "float",
      "required": false,
      "min": 1
    },
    "Id_recursos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recurso-proyecto.recurso-proyecto",
      "mappedBy": "id_proyecto"
    },
    "usuario_proyecto": {
      "type": "string"
    },
    "Id_comentario": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentario.comentario",
      "mappedBy": "Id_proyecto"
    },
    "SLA": {
      "type": "integer"
    },
    "Id_auditoria_proyectos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::auditoria-proyecto.auditoria-proyecto",
      "mappedBy": "id_proyectos"
    },
    "Id_auditoria_tareas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::audotira-tarea.audotira-tarea",
      "mappedBy": "id_proyectos"
    },
    "Id_respuestas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::respuesta.respuesta",
      "mappedBy": "Id_proyectos"
    }
  }
}
